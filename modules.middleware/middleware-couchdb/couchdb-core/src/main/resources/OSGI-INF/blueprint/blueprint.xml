<blueprint xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
           xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0
           http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">


    <service id="couchdbAttachBeanService" interface="cn.com.rexen.couchdb.api.biz.ICouchdbAttachBeanService"
             ref="couchdbAttachBeanServiceImpl">
    </service>

    <bean id="couchdbAttachBeanServiceImpl" class="cn.com.rexen.couchdb.core.biz.CouchdbAttachBeanServiceImpl">
        <property name="couchdbAttachBeanDao" ref="couchdbAttachBeanDao"/>
        <property name="couchdbService" ref="couchdbServiceImpl"/>
    </bean>

    <reference id="couchdbAttachBeanDao" interface="cn.com.rexen.couchdb.api.dao.ICouchdbAttachBeanDao"/>

    <service id="couchdbService" interface="cn.com.rexen.couchdb.api.biz.ICouchdbService" ref="couchdbServiceImpl">
    </service>

    <bean id="couchdbServiceImpl" class="cn.com.rexen.couchdb.core.biz.CouchdbServiceImpl" init-method="init">
    </bean>

</blueprint>